<!DOCTYPE html>
<html lang="en">
<head>
  <%include ../partials/head %>
</head>
<body class="container">
  <header>
    <%include ../partials/header %>
  </header>
  <main>
    <div class="row">
      <% jobs.forEach(function(job) { %>
      <div class="col-sm-6 col-md-4">
            <div class="caption">
              <h3><%= job.title %> <span class="label label-info"></h3>
              <p><%- job.description %></p>
              <p><a href="<%= job.apply_url %>" class="btn btn-primary" role="button">Apply</a></p>
            </div>
        </div>
      <% }); %>
    </div>
  </main>
  <footer>
    <%include ../partials/footer %>
  </footer>
</body>
</html>
